{
  "name": "snake-camel",
  "version": "1.0.8",
  "description": "convert object keys from/to camelCase and snake_case",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --maxWorkers=2 src",
    "build": "tsc --declaration"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:craftgear/snake-camel.git"
  },
  "keywords": [
    "camelCase",
    "snake_case"
  ],
  "author": "Shunsuke Watanabe <craftgear@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.1",
    "babel-jest": "^26.6.0",
    "jest": "^26.6.3",
    "prettier": "^1.19.1",
    "ts-jest": "^26.4.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    },
    "testPathIgnorePatterns": [
      "\\.snap$",
      "built",
      "<rootdir>/node_modules"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
